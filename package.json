{
  "name": "owner",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run generate && ng lint && ng serve",
    "build": "ng lint && ng build",
    "watch": "ng lint && ng build --watch --configuration dev",
    "test": "ng test",
    "dev:ssr": "npm run generate && ng lint && ng run owner:serve-ssr",
    "serve:ssr": "node dist/owner/server/main.js",
    "build:ssr": "npm run generate && ng lint && ng build && ng run owner:server",
    "prod-build:ssr": "npm run generate && ng lint && ng build --configuration production && ng run owner:server",
    "prerender": "ng run owner:prerender",
    "lint": "ng lint",
    "generate": "npm run generate-node",
    "generate-node": "ng-openapi-gen --input openapi/node-api.yaml --output ./src/app/node-api --ignoreUnusedModels false --configuration NodeApiConfiguration --module NodeApiModule"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/platform-server": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@nguniversal/express-engine": "^16.1.1",
    "@types/swiper": "^6.0.0",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "ng-zorro-antd": "^16.1.0",
    "ngx-cookie-service": "^16.0.0",
    "ngx-cookie-service-ssr": "^16.0.0",
    "ngx-device-detector": "^6.0.2",
    "rxjs": "~7.8.0",
    "swiper": "^11.0.3",
    "tslib": "^2.3.0",
    "typed.js": "^2.0.16",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.1.3",
    "@angular/compiler-cli": "^16.1.0",
    "@nguniversal/builders": "^16.1.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.44.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-openapi-gen": "0.23.0",
    "typescript": "~5.1.3"
  }
}
